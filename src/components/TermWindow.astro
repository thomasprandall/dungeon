---
import Window from "../layouts/Window.astro";
// Component Script (JavaScript)
const { title, listData, classes } = Astro.props;
---

<!-- Component Template (HTML + JS Expressions) -->
<Window title={title} classes={classes}>
  <div class="window-pane">
    {
      listData.map((item) => (
        <div class="">
          <>
            <h1 id={item.name}>{item.name}</h1>
            <p>{item.definition}</p>
          </>
        </div>
      ))
    }
  </div>
</Window>
